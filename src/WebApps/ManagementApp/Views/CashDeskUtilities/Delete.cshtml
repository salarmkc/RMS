@model ApplicationCore.ViewModel.CashDesk.CashDeskUtilityViewModel

@{
    ViewData["Title"] = "Delete";
}


<h3>تائید حذف</h3>
<div>
    <h4>CashDeskUtility</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CashDesk)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CashDesk.Name)
        </dd >
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Utility)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Utility.MainName)
        </dd >
        
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">بازگشت به لیست</a>
    </form>
</div>
